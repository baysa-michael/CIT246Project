<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs246.businesscalendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DailyCalendar"></activity>
        <activity android:name=".WeeklyCalendar" />
        <activity android:name=".MonthlyCalendar" />
        <activity android:name=".SelectView" />
        <activity android:name=".Appointment" />
        <activity android:name=".AddNew" />
        <activity android:name=".Landing" />
        <activity android:name=".Login">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Welcome" />
        </activity>
        <activity android:name=".CreateAccount">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Welcome" />
        </activity>
        <activity android:name=".Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
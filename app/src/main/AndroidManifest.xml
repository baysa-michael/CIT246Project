<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs246.businesscalendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view_presenter.daily_calendar.DailyCalendar" />
        <activity android:name=".view_presenter.weekly_calendar.WeeklyCalendar" />
        <activity android:name=".view_presenter.monthly_calendar.MonthlyCalendar" />
        <activity android:name=".view_presenter.select_view.SelectView" />
        <activity android:name=".view_presenter.modify_appointment.ModifyAppointment" />
        <activity android:name=".view_presenter.add_new.AddNew" />
        <activity android:name=".view_presenter.landing.Landing" />
        <activity android:name=".view_presenter.login.Login">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".view_presenter.welcome.Welcome" />
        </activity>
        <activity android:name=".view_presenter.create_account.CreateAccount">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".view_presenter.welcome.Welcome" />
        </activity>
        <activity android:name=".view_presenter.welcome.Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>